* {
  margin: 0;
  padding: 0;
  list-style: none;
  font-family: Microsoft YaHei;
  text-decoration: none;
  letter-spacing: 0.75px;
  font-weight: 400;
}
body {
  background: rgba(242, 242, 242, 1);
}
.banner {
  width: 100%;
  height: 602px;
  overflow: hidden;
  position: relative;
  font-size: 0;
  margin: 0 auto;
}
.b-img {
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
}
.b-img .img {
  display: block;
  height: 100%;
  float: left;
}
.b-list {
  height: 20px;
  padding-top: 570px;
  margin: 0 auto;
  position: relative;
  z-index: 1;
  width: 54px;
}
.b-list span {
  display: block;
  cursor: pointer;
  width: 8px;
  height: 8px;
  border-radius: 50%;
  background: #fff;
  float: left;
  margin: 0 5px;
  _margin: 0 3px;
}
.b-list .spcss {
  background: #d8241d;
}
section .account {
  background: rgba(31, 58, 94, 1);
  position: relative;
}
section .account ul li:first-child {
  background-image: url(../img/homePage/b5.png);
}
section .account ul li {
  padding-left: 65px;
  position: relative;
  background-position: 20px 26px;
  background-repeat: no-repeat;
  height: 118px;
  cursor: pointer;
}
section .account ul li p {
  font-size: 16px;
  color: rgba(255, 255, 255, 1);
  margin-left: 10px;
  min-width: 107px;
  margin-top: 18px;
  margin-bottom: 8px;
}
section .account ul li span:first-of-type {
  min-width: 107px;
  font-size: 12px;
  color: rgba(121, 175, 240, 1);
  display: block;
  line-height: 18px;
  letter-spacing: 0.5px;
  margin-left: 10px;
  margin-right: 15px;
}
section .account ul li span:nth-of-type(2) {
  width: 1px;
  height: 51px;
  background: rgba(41, 83, 139, 1);
  display: block;
  position: absolute;
  top: 34px;
  right: 0;
}
section .account ul li:nth-child(2) {
  background-image: url(../img/homePage/b6.png);
}
section .account ul li:nth-child(3) {
  background-image: url(../img/homePage/b3.png);
}
section .account ul li:nth-child(4) {
  background-image: url(../img/homePage/b2.png);
}
section .account ul li:nth-child(5) {
  background-image: url(../img/homePage/b4.png);
}
section .account ul li:nth-child(6) {
  background-image: url(../img/homePage/b1.png);
}
section .preponderance {
