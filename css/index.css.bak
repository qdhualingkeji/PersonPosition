  left: 568px;
  top: 212px;
}
section .services {
  overflow: hidden;
  background: rgba(255, 255, 255, 1);
  height: 830px;
}
section .services .bg {
  background: url(../img/homePage/fuwuL.png) center no-repeat;
  height: 523px;
  overflow: hidden;
}
section .services .width {
  position: relative;
  width: 1250px;
}
section .services .bg p:first-of-type {
  font-size: 30px;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  margin-top: 140px;
  margin-bottom: 40px;
}
section .services .bg p:nth-of-type(2) {
  font-size: 18px;
  color: rgba(255, 255, 255, 1);
  text-align: center;
}
section .services .width .position {
  position: relative;
  left: 0px;
  top: -212px;
}

section .services .width .position ul li .around {
  width: 90%;
  height: 399px;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 12px 27px 0px rgba(33, 15, 121, 0.14);
  transition: 0.6s;
  margin: auto;
  overflow: hidden;
}
section .services .width .position ul li .around:hover {
  transform: translateY(-15px);
}
section .services .width .position ul li .around > span {
  height: 105px;
  display: block;
  padding: 28px 0px 24px 0px;
  background-repeat: no-repeat;
  background-position: center center;
}
section .services .width .position ul li .around::after {
  content: "";
  width: 100%;
  position: absolute;
  background: #ff9900;
  opacity: 0.8;
  bottom: 0;
  height: 0;
  transition: height 0.2s linear;
  z-index: 555;
  margin: auto;
}
section .services .width .position ul li .around:hover::after {
  height: 2px;
}
section .services .width .position ul li:first-of-type .around > span {
  background-image: url(../img/homePage/hardware-serverL.png);
}
section .services .width .position ul li:nth-of-type(2) .around > span {
  background-image: url(../img/homePage/software-serverL.png);
}
section .services .width .position ul li:nth-of-type(3) .around > span {
  background-image: url(../img/homePage/scheme-serverL.png);
}
section .services .width .position ul li:nth-of-type(4) .around > span {
  background-image: url(../img/homePage/operation-serverL.png);
}
section .services .width .position ul li .around > p {
  font-size: 18px;
  color: rgba(46, 46, 46, 1);
  text-align: center;
  margin-bottom: 30px;
  font-family: SimHei;
}
section .services .width .position ul li .around div p {
  font-size: 14px;
  color: rgba(102, 102, 102, 1);
  text-align: center;
  line-height: 30px;
}
section .address {
