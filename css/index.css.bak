  height: 40px;
}
section .bgp .width .frist li div p:first-of-type {
  font-size: 28px;
  color: rgba(86, 86, 86, 1);
  letter-spacing: -0.25px;
  margin-left: 18px;
  margin-bottom: 10px;
}
section .bgp .width .frist li div p:last-of-type {
