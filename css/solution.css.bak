body{
	overflow:-Scroll;
	overflow-x:hidden;
}
.logo_div{
	width:100%;height:460px;overflow-y:hidden;
}
.logo_div .logo_img{
	width:100%;
}
.logo_div .content_div{
	width:700px;margin:-578px auto 0;color:#fff;text-align: center;
}
.logo_div .title_div{
	font-weight: 500;font-size: 2.5rem;
}
.logo_div .describe_div{
	font-weight: 500;font-size: 1.3rem;line-height: 40px;margin-top:30px;
}

.uwbjjfa_div{
	width:1120px;height:350px;margin:40px auto 0;
}
.uwbjjfa_div .uwbjjfa_img{
	width:1120px;height:350px;
}
.uwbjjfa_div .content_div{
	width:410px;height:122px;color:#fff;float:right;margin-top: -240px;margin-right: 50px;
}
.uwbjjfa_div .title_div{
	width:410px;height:50px;line-height:50px;font-size:23px;font-weight:bold;border-bottom: 4px solid #fff;position: absolute;
}
.uwbjjfa_div .describe_div{
	width:400px;line-height:28px;font-size:15px;margin-top: 58px;position: absolute;
}
.uwbjjfa_div .describe_div .yycj_span{
	font-weight:bold;
}

.fags_div,.xtjg_div,.ywgn_div{
	width:1120px;margin:40px auto 0;
}
.fags_div .title_div,.xtjg_div .title_div,.ywgn_div .title_div{
	width:100%;
}
.fags_div .title_img,.xtjg_div .title_img,.ywgn_div .title_img{
	width:15px;height:15px;
}
.fags_div .title_span,.xtjg_div .title_span,.ywgn_div .title_span{
	margin-left:3px;color: #2169e0;font-size:23px;font-weight:bolder;
}
.fags_div .describe_div,.xtjg_div .describe_div{
	line-height:50px;font-size:25px;text-align:center;
}
.fags_div .lsdz_span{
	color:#60C067;
}
.fags_img,.xtjg_img{
	width:100%;margin-top:10px;
}

.ywgn_div .list_div{
	margin-bottom:20px;
}
.ywgn_div .list_div .item_div{
	margin-top:30px;
}
.ywgn_div .list_div .item_div .title_div{
	color: #2169e0;font-size: 14pt;font-weight: bolder;
}
.ywgn_div .list_div .item_div img{
	width:100%;margin-top:5px;
}

.foot_div{
	width:100%;height:237px;background-color:#1F232A;
}
.foot_div .content_div{
	width:1340px;height:185px;margin:0 auto;
}
.foot_div .space_div{
	height:25px;
}
.foot_div .row_div{
	font-size: 15px;color: #FFFFFF;line-height: 45px;letter-spacing: 0;
}
.foot_div .bah_div{
	width:1440px;
	font-size: 14px;
	color: #999;
	text-align: center;
	letter-spacing: 0;
	margin:0 auto;
	padding-top:15px;
	border-top:#999 solid 1px;
}